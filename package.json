{
  "name": "jigsawstack",
  "description": "JigsawStack - The AI SDK for Typescript and Javascript",
  "keywords": ["framework", "ai", "nextjs", "react"],
  "type": "module",
  "license": "MIT",
  "version": "0.2.6",
  "homepage": "https://jigsawstack.com",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JigsawStack/jigsawstack-js.git"
  },
  "bugs": {
    "url": "https://github.com/JigsawStack/jigsawstack-js/issues"
  },
  "packageManager": "yarn@1.22.22",
  "engines": {
    "node": ">=18"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.cts",
  "files": ["dist/**"],
  "exports": {
    "require": {
      "types": "./dist/index.d.cts",
      "default": "./dist/index.cjs"
    },
    "import": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    }
  },
  "scripts": {
    "build": "pkgroll --src=./",
    "format": "biome check --write ."
  },
  "dependencies": {
    "isomorphic-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/node": "^22.0.2",
    "pkgroll": "^2.11.2",
    "tsx": "^4.16.4",
    "typescript": "^5.5.4"
  }
}
